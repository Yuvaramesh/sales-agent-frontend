{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "static/script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/styles.css",
      "dest": "/static/styles.css"
    },
    {
      "src": "/static/script.js",
      "dest": "/static/script.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "BACKEND_URL": "https://sales-backend-sepia.vercel.app/"
  }
}
